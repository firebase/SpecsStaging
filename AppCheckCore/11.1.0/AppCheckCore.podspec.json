{
  "name": "AppCheckCore",
  "version": "11.1.0",
  "summary": "App Check Core SDK.",
  "description": "SDK for anti-abuse compatibility.",
  "homepage": "https://firebase.google.com",
  "license": {
    "type": "Apache-2.0",
    "file": "LICENSE"
  },
  "authors": "Google, Inc.",
  "source": {
    "git": "https://github.com/google/app-check.git",
    "tag": "CocoaPods-11.1.0"
  },
  "social_media_url": "https://twitter.com/Firebase",
  "swift_versions": "5.5",
  "platforms": {
    "ios": "12.0",
    "osx": "10.15",
    "tvos": "13.0",
    "watchos": "7.0"
  },
  "cocoapods_version": ">= 1.4.0",
  "prefix_header_file": false,
  "source_files": [
    "AppCheckCore/Sources/**/*.[mh]"
  ],
  "public_header_files": "AppCheckCore/Sources/Public/AppCheckCore/*.h",
  "ios": {
    "weak_frameworks": "DeviceCheck"
  },
  "osx": {
    "weak_frameworks": "DeviceCheck"
  },
  "tvos": {
    "weak_frameworks": "DeviceCheck"
  },
  "dependencies": {
    "PromisesObjC": [
      "~> 2.4"
    ],
    "GoogleUtilities/Environment": [
      "~> 8.0"
    ],
    "GoogleUtilities/UserDefaults": [
      "~> 8.0"
    ]
  },
  "pod_target_xcconfig": {
    "GCC_C_LANGUAGE_STANDARD": "c99",
    "HEADER_SEARCH_PATHS": "\"${PODS_TARGET_SRCROOT}\""
  },
  "testspecs": [
    {
      "name": "unit",
      "test_type": "unit",
      "platforms": {
        "ios": "12.0",
        "osx": "10.15",
        "tvos": "13.0"
      },
      "source_files": [
        "AppCheckCore/Tests/Unit/**/*.[mh]",
        "AppCheckCore/Tests/Utils/**/*.[mh]"
      ],
      "resources": "AppCheckCore/Tests/Fixture/**/*",
      "dependencies": {
        "OCMock": [

        ]
      },
      "requires_app_host": true
    },
    {
      "name": "integration",
      "test_type": "unit",
      "platforms": {
        "ios": "12.0",
        "osx": "10.15",
        "tvos": "13.0"
      },
      "source_files": [
        "AppCheckCore/Tests/Integration/**/*.[mh]",
        "AppCheckCore/Tests/Integration/**/*.[mh]"
      ],
      "resources": "AppCheckCore/Tests/Fixture/**/*",
      "requires_app_host": true
    },
    {
      "name": "swift-unit",
      "test_type": "unit",
      "platforms": {
        "ios": "12.0",
        "osx": "10.15",
        "tvos": "13.0"
      },
      "source_files": [
        "AppCheckCore/Tests/Unit/Swift/**/*.swift",
        "AppCheckCore/Tests/Unit/Swift/**/*.h"
      ]
    }
  ],
  "swift_version": "5.5"
}
